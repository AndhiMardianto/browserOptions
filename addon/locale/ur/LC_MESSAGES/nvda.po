#: globalPlugins/browserOptions/__init__.py:56
#: globalPlugins/browserOptions/__init__.py:107
msgid "Add Favorite"
msgstr "پسندیدہ شامل کریں"

#: globalPlugins/browserOptions/__init__.py:60
msgid "Name:"
msgstr "نام:"

#: globalPlugins/browserOptions/__init__.py:61
msgid "URL:"
msgstr "یو آر ایل:"

#: globalPlugins/browserOptions/__init__.py:66
msgid "Save"
msgstr "محفوظ کریں"

#: globalPlugins/browserOptions/__init__.py:79
msgid "Both Name and URL are required."
msgstr "نام اور یو آر ایل دونوں ضروری ہیں۔"

#: globalPlugins/browserOptions/__init__.py:83
msgid "Invalid URL. It should start with http:// or https://"
msgstr "غلط یو آر ایل۔ اسے http:// یا https:// سے شروع ہونا چاہیے۔"

#: globalPlugins/browserOptions/__init__.py:89
msgid "Favorite added successfully!"
msgstr "پسندیدہ کامیابی کے ساتھ شامل کر دیا گیا!"

#: globalPlugins/browserOptions/__init__.py:98
msgid "Input URL or Keywords"
msgstr "یو آر ایل یا مطلوبہ الفاظ درج کریں"

#: globalPlugins/browserOptions/__init__.py:101
msgid "Search"
msgstr "تلاش کریں"

#: globalPlugins/browserOptions/__init__.py:104
msgid "Favorites"
msgstr "پسندیدہ"

#: globalPlugins/browserOptions/__init__.py:110
msgid "Delete Favorite"
msgstr "پسندیدہ کو حذف کریں"

#: globalPlugins/browserOptions/__init__.py:124
msgid "Input cannot be empty"
msgstr "درج کردہ مواد خالی نہیں ہو سکتا۔"

#: globalPlugins/browserOptions/__init__.py:139
msgid "No browser detected"
msgstr "کوئی براؤزر نہیں ملا۔"

#: globalPlugins/browserOptions/__init__.py:143
msgid "Select Browser"
msgstr "براؤزر منتخب کریں"

#: globalPlugins/browserOptions/__init__.py:182
msgid "No favorites found"
msgstr "کوئی پسندیدہ نہیں ملا۔"

#: globalPlugins/browserOptions/__init__.py:187
msgid "Select Favorite"
msgstr "پسندیدہ منتخب کریں"

#: globalPlugins/browserOptions/__init__.py:208
msgid "Select Favorite Link to Delete"
msgstr "حذف کرنے کے لئے پسندیدہ لنک منتخب کریں۔"

#: globalPlugins/browserOptions/__init__.py:214
msgid "Favorite deleted successfully!"
msgstr "پسندیدہ کامیابی کے ساتھ حذف کر دیا گیا!"

msgid "Ok"
msgstr "ٹھیک ہے"

msgid "Cancel"
msgstr "منسوخ کریں"
